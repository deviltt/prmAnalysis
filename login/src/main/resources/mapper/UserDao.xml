<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tt.oa.dao.UserDao">
    <select id="getUser" resultType="com.tt.oa.entity.User">
        select * from oa1.user where username=#{username}
    </select>
</mapper>